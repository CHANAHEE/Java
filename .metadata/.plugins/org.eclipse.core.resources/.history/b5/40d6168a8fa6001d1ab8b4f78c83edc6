import javax.swing.JFrame;
import javax.swing.JTextField;

public class MyFrame extends JFrame{
	
	// 2_ 생성자 메소드
	public MyFrame() {
		// 3_ 객체가 생성될 때 본인의 설정을 모두 해놓기
		setSize(500, 500);
		setDefaultCloseOperation(EXIT_ON_CLOSE);
		setVisible(true);
		setLocation(750, 250);
		JTextField tf = new JTextField("dd",2);
		tf.setVisible(true);
		add(tf);
	}
}
