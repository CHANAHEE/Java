
public class JoinTest {
	public static void main(String[] args) {
		
	}
}

// 1_ 클래스 생성
class JThread extends Thread {
	@Override
	public void run() {
		for(int i = 0; i < 10; i++) {
			System.out.println("aaa");
			try {
				sleep(500);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
		}
	}
}