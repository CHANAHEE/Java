
public class RaceHorse {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
	}

}

// 1_ 말 클래스 만들기
class Horse extends Thread {
	@Override
	public void run() {
		for(int i = 0; i < 20; i++) {
			System.out.println("다그닥! 다그닥!");
			
			try {
				Thread.sleep(400);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
		}
	}
}
